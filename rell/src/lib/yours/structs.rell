struct token_balance_dto {
  project: project_info;
  collection: name;
  token_id: big_integer;
  uid: byte_array;
  type: text;
  amount: big_integer;
}

struct token_identifier {
  project: project_info;
  collection: name;
  token_id: big_integer;
}

struct page_cursor {
  before_rowid: rowid? = null;
}

struct pagination_result {
  data: gtv;
  rowid: rowid;
}

struct paged_result {
  next_cursor: text?;
  data: list<gtv>;
}
