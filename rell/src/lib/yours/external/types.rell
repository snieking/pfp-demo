struct mint_message {
  project: name;
  collection: name;
  token_id: big_integer;
  to: byte_array;
  amount: big_integer;
  chain: name;
  contract: byte_array;
}

struct transfer_message {
  chain: name;
  contract: byte_array;
  token_id: big_integer;
  from: byte_array;
  to: byte_array;
  amount: big_integer;
}

struct metadata_updated_message {
  chain: name;
  contract: byte_array;
  token_id: big_integer;
  metadata: json;
}
